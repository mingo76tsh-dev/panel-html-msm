{
  "id": "/panel-html-msm/",
  "name": "HSM v7 • Móvil",
  "short_name": "HSM Móvil",
  "description": "Ingreso y alta rápida con soporte offline y cola de pendientes.",
  "lang": "es-AR",
  "dir": "ltr",

  "start_url": "/panel-html-msm/?source=pwa",
  "scope": "/panel-html-msm/",
  "display": "standalone",
  "display_override": ["window-controls-overlay","standalone","minimal-ui","browser"],
  "orientation": "portrait-primary",

  "background_color": "#0b1220",
  "theme_color": "#0b1220",
  "categories": ["health","medical","productivity","utilities"],
  "prefer_related_applications": false,

  "launch_handler": { "client_mode": "navigate-existing" },

  "icons": [
    { "src": "/panel-html-msm/icons/icon-192.png",     "sizes": "192x192", "type": "image/png", "purpose": "any" },
    { "src": "/panel-html-msm/icons/icon-512.png",     "sizes": "512x512", "type": "image/png", "purpose": "any" },
    { "src": "/panel-html-msm/icons/maskable-192.png", "sizes": "192x192", "type": "image/png", "purpose": "maskable" },
    { "src": "/panel-html-msm/icons/maskable-512.png", "sizes": "512x512", "type": "image/png", "purpose": "maskable" },
    { "src": "/panel-html-msm/icons/apple-touch-icon.png", "sizes": "180x180", "type": "image/png", "purpose": "any" }
  ],

  "shortcuts": [
    { "name": "Nuevo ingreso", "short_name": "Ingreso", "url": "/panel-html-msm/#ing",  "description": "Ir directo a Ingreso" },
    { "name": "Dar alta",      "short_name": "Alta",    "url": "/panel-html-msm/#alta", "description": "Ir directo a Alta" },
    { "name": "Pendientes",    "short_name": "Pend.",   "url": "/panel-html-msm/#pend", "description": "Ver pendientes offline" }
  ],

  "protocol_handlers": [
    { "protocol": "web+hsm", "url": "/panel-html-msm/?deeplink=%s" }
  ],

  "screenshots": [
    {
      "src": "/panel-html-msm/icons/screen-1080x1920.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/panel-html-msm/icons/screen-1920x1080.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide"
    }
  ]
}
